{
  "name": "rewolf-trans",
  "repository": "github:KCFindstr/rewolf-trans",
  "version": "0.1.5",
  "description": "Translate Wolf RPG games.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "scripts": {
